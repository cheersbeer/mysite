{
  "name": "mysite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "yarn prettier -w .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "test": "echo no test specified",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "@commitlint/cli/**/yargs-parser": "^18.1.3",
    "@gridsome/transformer-remark/**/minimist": "^1.2.5",
    "@vue/cli-plugin-unit-jest/**/yargs-parser": "^18.1.3"
  },
  "dependencies": {
    "@astrojs/sitemap": "^0.1.0",
    "@astrojs/tailwind": "^0.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@sarissa/pagination": "^1.0.2",
    "daisyui": "^2.13.6",
    "fuse.js": "^6.5.3",
    "luxon": "^2.3.1",
    "theme-change": "^2.0.2",
    "twemoji": "^13.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^13.1.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/jest": "^27.0.1",
    "@types/twemoji": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "astro": "^1.0.0-rc.7",
    "core-js": "^3.16.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "5",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "netlify-cli": "^10.7.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.3.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^27.0.4",
    "ts-loader": "8.4.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=12.x"
  }
}
