{
  "name": "mysite",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "yarn prettier -w .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "test": "echo no test specified",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "@commitlint/cli/**/yargs-parser": "^18.1.3",
    "@gridsome/transformer-remark/**/minimist": "^1.2.5",
    "@vue/cli-plugin-unit-jest/**/yargs-parser": "^18.1.3"
  },
  "dependencies": {
    "@astrojs/sitemap": "^1.3.3",
    "@astrojs/tailwind": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@sarissa/pagination": "^1.0.3",
    "daisyui": "^3.1.1",
    "fuse.js": "^6.6.2",
    "luxon": "^3.3.0",
    "theme-change": "^2.5.0",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.5.2",
    "@types/twemoji": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.8",
    "astro": "^2.6.3",
    "autoprefixer": "^10.4.14",
    "core-js": "^3.31.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "netlify-cli": "^15.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "9.4.2",
    "typescript": "^5.1.3",
    "yarn-audit-fix": "^9.3.12"
  },
  "engines": {
    "node": ">=18.x"
  }
}
