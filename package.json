{
  "name": "mysite",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "yarn prettier -w .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "test": "echo no test specified",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "@commitlint/cli/**/yargs-parser": "^18.1.3",
    "@gridsome/transformer-remark/**/minimist": "^1.2.5",
    "@vue/cli-plugin-unit-jest/**/yargs-parser": "^18.1.3"
  },
  "dependencies": {
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^2.1.3",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@sarissa/pagination": "^1.0.3",
    "daisyui": "^2.46.1",
    "fuse.js": "^6.6.2",
    "luxon": "^3.2.1",
    "theme-change": "^2.3.0",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.2.5",
    "@types/twemoji": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.47.1",
    "astro": "^1.9.2",
    "autoprefixer": "^10.4.13",
    "core-js": "^3.27.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "netlify-cli": "^12.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prettier-plugin-astro": "^0.7.2",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "9.4.2",
    "typescript": "^4.9.4",
    "yarn-audit-fix": "^9.3.7"
  },
  "engines": {
    "node": ">=14.x"
  }
}
